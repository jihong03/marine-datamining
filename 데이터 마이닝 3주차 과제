      "cell_type": "code",
      "source": [
        "menu = {\n",
        "    \"Americano\": 3000,\n",
        "    \"Ice Americano\": 3500,\n",
        "    \"Cappuccino\": 4000,\n",
        "    \"Cafe Latte\": 4500,\n",
        "    \"Espresso\": 3600\n",
        "}\n",
        "\n",
        "for key in menu:\n",
        "    print(\"{:16s}  가격 : {:,}원\".format(key, menu[key]))\n",
        "\n",
        "choice = input(\"위의 메뉴 중 하나를 선택하세요: \")\n",
        "\n",
        "if choice in menu:\n",
        "    print(\"{}는 {:,}원 입니다. 결제를 부탁합니다.\".format(choice, menu[choice]))\n",
        "else:\n",
        "    print(\"미안합니다. {}는 메뉴에 없습니다.\".format(choice))"
      ],
